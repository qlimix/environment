{
    "name": "qlimix/environment",
    "type": "library",
    "description": "Environment utilities",
    "license": "MIT",
    "keywords": [
        "environment",
        "env"
    ],
    "require": {
        "php": ">7.4"
    },
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Qlimix\\Environment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\Environment\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^3.0"
    }
}
